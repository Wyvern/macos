MacOS:

sh -c "$(curl -fsSL https://raw.github.com/robbyrussell/oh-my-zsh/master/tools/install.sh)"
/usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"

chsh -s /bin/zsh

csrutil disable; csrutil enable
reboot
sudo spctl --master-disable

brew cask install mpv

chflags uchg  /private/var/db/analyticsd
chflags uchg  /private/var/db/diagnostics
chflags uchg  /private/var/db/reportmemoryexception
chflags uchg  /var/log
chflags uchg  /var/vm
chflags uchg  /Library/Logs/
chflags uchg  ~/Library/Logs/

defaults write NSGlobalDomain NSAutomaticWindowAnimationsEnabled -bool false
defaults write -g NSWindowResizeTime -float 0.001
defaults write -g NSToolbarFullScreenAnimationDuration -float 0
